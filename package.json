{
  "name": "minanft",
  "description": "MinaNFT TypeScript/JavaScript integration library",
  "keywords": [
    "minanft",
    "NFT",
    "library",
    "TypeScript",
    "mina",
    "zkapp",
    "zk",
    "blockchain",
    "web3",
    "zero knowledge"
  ],
  "version": "1.0.10",
  "repository": "git@dfstio.github.com:dfstio/minanft-lib.git",
  "main": "src/minanft.ts",
  "author": "dfst.io",
  "license": "Apache-2.0",
  "scripts": {
    "compile": "rm -rf dist/lib && tsc && tsc --build tsconfig.js.json",
    "archive": "git archive -o lib.zip HEAD",
    "lint": "eslint src/*.ts",
    "testold": "ts-node tests/test.ts",
    "test": "jest",
    "deployers": "ts-node experimental/deployers.ts",
    "coverage": "jest --coverage",
    "style": "prettier --tab-width 4 --write {,!(node_modules)/**/}*.ts",
    "push": "git status && git push -u originminanftlib recursive",
    "gitcreate": "git remote add originminanftlib https://github.com/dfstio/minanft-lib",
    "githubcleartoken": "git config --global --unset credential.helper",
    "checkout": "git checkout -b lib",
    "upgradeinteractive": "yarn upgrade-interactive",
    "commit": "git commit -a -S -m",
    "login": "yarn npm login",
    "publish": "yarn npm publish --access public",
    "docs": "yarn typedoc",
    "vscode": "yarn dlx @yarnpkg/sdks vscode"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cli-progress": "^3.12.0",
    "docs": "^0.3.2-canary.0",
    "o1js": "^0.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@jest/globals": "^29.7.0",
    "@mxssfd/typedoc-theme": "^1.1.3",
    "@types/cli-progress": "^3.11.3",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.1",
    "typedoc": "0.25.3",
    "typedoc-plugin-mermaid": "^1.10.0",
    "typescript": "5.2.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {}
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "slowTestThreshold": 1500,
    "verbose": true,
    "roots": [
      "tests",
      "experimental"
    ],
    "setupFilesAfterEnv": [
      "./jest-config.ts"
    ]
  },
  "packageManager": "yarn@3.6.4"
}
