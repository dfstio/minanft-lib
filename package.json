{
  "name": "minanft",
  "description": "MinaNFT TypeScript/JavaScript integration library",
  "keywords": [
    "minanft",
    "NFT",
    "library",
    "TypeScript",
    "mina",
    "zkapp",
    "zk",
    "blockchain",
    "web3",
    "zero knowledge"
  ],
  "version": "1.1.9",
  "repository": "git@dfstio.github.com:dfstio/minanft-lib.git",
  "main": "src/index.ts",
  "author": "dfst.io",
  "license": "Apache-2.0",
  "scripts": {
    "compile": "rm -rf dist/lib && tsc && tsc --build tsconfig.js.json",
    "compilets": "rm -rf dist/lib && tsc",
    "test": "jest",
    "deployers": "ts-node experimental/deployers.ts",
    "await": "ts-node experimental/await.ts",
    "testworld2": "ts-node experimental/testworld2.ts",
    "coverage": "jest --coverage",
    "githubcleartoken": "git config --global --unset credential.helper",
    "upgradeinteractive": "yarn upgrade-interactive",
    "login": "yarn npm login",
    "publish": "cd dist && yarn npm publish --access public",
    "docs": "yarn typedoc",
    "vscode": "yarn dlx @yarnpkg/sdks vscode"
  },
  "dependencies": {
    "axios": "^1.6.1",
    "cli-progress": "^3.12.0",
    "form-data": "^4.0.0",
    "mime": "^3.0.0",
    "o1js": "^0.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@mxssfd/typedoc-theme": "^1.1.3",
    "@types/cli-progress": "^3.11.5",
    "@types/jest": "^29.5.8",
    "@types/mime": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.1",
    "typedoc": "0.25.3",
    "typedoc-plugin-mermaid": "^1.10.0",
    "typescript": "5.2.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {}
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "slowTestThreshold": 1500,
    "testTimeout": 10800000,
    "verbose": true,
    "roots": [
      "tests",
      "experimental"
    ],
    "setupFilesAfterEnv": [
      "./jest-config.ts"
    ]
  },
  "packageManager": "yarn@3.6.4"
}
